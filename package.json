{
  "name": "gatsby-source-google-docs-sheets",
  "version": "2.0.0",
  "description": "Gatsby plugin to use Google Docs & Sheets as a data source",
  "repository": "https://github.com/rishabh09/gatsby-source-google-docs",
  "author": {
    "name": "Rishabh Mishra",
    "email": "me@rishabhmishra.dev"
  },
  "license": "MIT",
  "private": false,
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source",
    "gatsby-source-plugin",
    "google",
    "google-docs",
    "docs",
    "drive"
  ],
  "main": "index.js",
  "dependencies": {
    "gatsby-source-filesystem": "^2.1.48",
    "googleapis": "47.0.0",
    "inquirer": "^7.0.4",
    "json2md": "^1.6.2",
    "lodash": "^4.17.13",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.7.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "1.19.1"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0"
  },
  "scripts": {
    "clean": "rm -rf ./gatsby-node.js ./utils",
    "prebuild": "yarn clean",
    "build": "cross-env BABEL_ENV=production babel src --out-dir . --ignore 'src/__tests__/*'",
    "lint": "eslint --ext .js src",
    "test": "jest",
    "postinstall": "npm run build",
    "prepublishOnly": "yarn build",
    "token": "./scripts/generate-token.js"
  },
  "bin": {
    "gatsby-source-google-docs-token": "./scripts/generate-token.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
